  <property>
    <name>yarn.nm.liveness-monitor.expiry-interval-ms</name>
    <value>15000</value>
  </property>
  <property>
    <name>yarn.resourcemanager.webapp.cross-origin.enabled</name>
    <value>true</value>
  </property>
  <property>
    <name>yarn.timeline-service.http-cross-origin.enabled</name>
    <value>false</value>
